{
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "printWidth": 120,
  "tabWidth": 2,
  "proseWrap": "never",
  "importOrder": [
    "^(react|react-dom)$",
    "^([a-z]|@[a-z])",
    "",
    ".*"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
